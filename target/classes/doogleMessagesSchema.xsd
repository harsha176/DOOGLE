<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.doogle.project.csc573.csc.ncsu.edu" xmlns="http://www.doogle.project.csc573.csc.ncsu.edu"
	elementFormDefault="qualified">
	<xs:complexType name="Request">
		<xs:sequence>
			<xs:element name="Command" type="CommandType"></xs:element>
		</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required">
		</xs:attribute>
	</xs:complexType>
        
        <xs:complexType name="Response">
		<xs:sequence>
			<xs:element name="Command" type="CommandType"></xs:element>
		</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required">
		</xs:attribute>
	</xs:complexType>
        
        
        

    <xs:complexType name="CommandType">
                <xs:choice>
			<xs:element name="Register" type="RegisterType"></xs:element>
			<xs:element name="Login" type="LoginType"></xs:element>
			<xs:element name="Logout" type="LogoutType"></xs:element>
			<xs:element name="ForgotPWD" type="ForgotPWDType"></xs:element>
			<xs:element name="Search" type="SearchType"></xs:element>
                        <xs:element name="Register Response" type="RegisterResponseType"></xs:element>
			<xs:element name="Login Response" type="LoginResponseType"></xs:element>
			<xs:element name="Logout Response" type="LogoutResponseType"></xs:element>
			<xs:element name="ForgotPWD Response" type="ForgotPWDResponseType"></xs:element>
			<xs:element name="Search Response" type="SearchResponseType"></xs:element>
		</xs:choice>
    </xs:complexType>
    
    
    <xs:complexType name="RegisterType">
    	<xs:sequence>
        <xs:element name="Operation" type="xs:string" fixed="Register"/>
    	<xs:element name="params" type="RegisterParamsType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="RegisterParamsType">
    	<xs:sequence>
    		<xs:element name="username" type="xs:string"/>
    		<xs:element name="email_id" type="xs:string"/>
    		<xs:element name="password" type="xs:string"/>
    		<xs:element name="designation" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    
    
    <xs:complexType name="RegisterResponseType">
    	<xs:sequence>
        <xs:element name="Operation" type="xs:string" fixed="Register Response"/>
    	<xs:element name="params" type="RegisterResponseParamsType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="RegisterResponseParamsType">
    	<xs:sequence>
    		<xs:element name="status code" type="xs:string"/>
    		<xs:element name="message" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>    
    
    
    
    <xs:complexType name="LoginType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Login"/>
    	<xs:element name="params" type="LoginParamsType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="LoginParamsType">
    	<xs:sequence>
    		<xs:element name="username" type="xs:string"/>
    		<xs:element name="password" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    
    
    <xs:complexType name="LoginResponseType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Login Response"/>
    	<xs:element name="params" type="LoginParamsType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="LoginResponseParamsType">
    	<xs:sequence>
    		<xs:element name="status code" type="xs:string"/>
    		<xs:element name="message" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType> 
    
    
    <xs:complexType name="LogoutType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Logout"/>
    	<xs:element name="params" type="LogoutParamsType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="LogoutParamsType">
    	<xs:sequence>
    		<xs:element name="username" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    
    
    
    <xs:complexType name="LogoutResponseType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Logout Response"/>
    	<xs:element name="params" type="LogoutResponseParamsType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="LogoutResponseParamsType">
    	<xs:sequence>
    		<xs:element name="status code" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    
    
    
   
    <xs:complexType name="ForgotPasswdType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="ForgotPWD"/>
    	<xs:element name="parmas" type="ForgotPWDType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="ForgotPWDType">
    	<xs:sequence>
    		<xs:element name="username" type="xs:string"/>
    		<xs:element name="email_id" type="xs:string"/>
    		<xs:element name="password" type="xs:string"/>
    		<xs:element name="designation" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    
    
<xs:complexType name="ForgotPasswdResponseType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="ForgotPWD Response"/>
    	<xs:element name="params" type="ForgotPWDResponseType">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="ForgotPWDResponseType">
    	<xs:sequence>
    		<xs:element name="status code" type="xs:string"/>
    		<xs:element name="message" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    
    
    
    <xs:complexType name="SearchType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Search"/>
    	<xs:element name="params" type="SearchTypeParams">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="SearchTypeParams">
    	<xs:sequence>
    		<xs:element name="username" type="xs:string"/>
    		<xs:element name="search" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>

    
    <xs:complexType name="SearchResponseType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Search Response"/>
    	<xs:element name="params" type="SearchResponseTypeParams" maxOccurs="100">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="SearchResponseTypeParams">
    	<xs:sequence>
    		<xs:element name="file name" type="xs:string"/>
    		<xs:element name="abstract" type="xs:string"/>
                <xs:element name="file size" type="xs:string"/>
                <xs:element name="ip address" type="xs:string"/>
                <xs:element name="file digest" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    
    
    <xs:complexType name="PublishType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Publish"/>
    	<xs:element name="params" type="PublishResponseTypeParams" maxOccurs ="100">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="PublishResponseTypeParams">
    	<xs:sequence>
    		<xs:element name="file name" type="xs:string"/>
    		<xs:element name="abstract" type="xs:string"/>
                <xs:element name="file size" type="xs:string"/>
                <xs:element name="ip address" type="xs:string"/>
                <xs:element name="file digest" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>
    

    
    <xs:complexType name="PublishResponseType">
    	<xs:sequence>
    	<xs:element name="Operation" type="xs:string" fixed="Publish Response"/>
    	<xs:element name="params" type="PublishResponseTypeParams">	
    	</xs:element>
    	</xs:sequence>
    </xs:complexType>
    <xs:complexType name="PublishResponseTypeParams">
    	<xs:sequence>
    		<xs:element name="status code" type="xs:string"/>
    		<xs:element name="message" type="xs:string"/>
    	</xs:sequence>
    </xs:complexType>    
    
</xs:schema> 